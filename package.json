{
  "name": "@lhlyu/zipson",
  "description": "A compact JSON-compatible serializer with compression and streaming support. 一个紧凑的 JSON 兼容序列化工具，支持压缩与流式处理。",
  "version": "0.0.1-beta.1",
  "homepage": "https://github.com/lhlyu/zipson",
  "bugs": {
    "url": "https://github.com/lhlyu/zipson/issues"
  },
  "license": "MIT",
  "author": "lhlyu",
  "repository": "https://github.com/lhlyu/zipson.git",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "types",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "fmt": "oxfmt",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "test": "vitest run",
    "build": "rolldown -c && tsc -p tsconfig.types.json"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "dummy-json": "^3.0.5",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "rolldown": "^1.0.0-rc.1",
    "rolldown-plugin-dts": "^0.21.7",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
